    {
      "id": "OLGg6NiU1u3otrmS_g9WR",
      "type": "text",
      "x": 633.357821433941,
      "y": 1743.6990022326383,
      "width": 597.9034423828125,
      "height": 640,
      "angle": 0,
      "strokeColor": "#1e1e1e",
      "backgroundColor": "transparent",
      "fillStyle": "solid",
      "strokeWidth": 2,
      "strokeStyle": "solid",
      "roughness": 1,
      "opacity": 100,
      "groupIds": [],
      "frameId": null,
      "index": "b4b",
